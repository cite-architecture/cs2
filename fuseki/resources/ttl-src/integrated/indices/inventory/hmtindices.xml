<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="CiteIndexInventory.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<indexInventory xmlns="http://chs.harvard.edu/xmlns/cite">
    <rdfNamespace abbr="hmt" fullValue="http://www.homermultitext.org/hmt/rdf/"/>
    <rdfNamespace abbr="cite" fullValue="http://www.homermultitext.org/cite/rdf/"/>
    <rdfNamespace abbr="dse" fullValue="http://www.homermultitext.org/dse/rdf/"/>




    <!-- ******* Fundamental DSE relations, 1:  TEXT -> TEXT-BEARING SURFACE *********** -->


    <index verb="dse:appearsOn" inverse="dse:hasOnIt">
        <source type="file" value="venA-Iliad-text2surface.csv"/>
    </index>





    <!--** end TEXT -> TEXT-BEARING SURFACE ********************************************* -->









    <!-- ******* Fundamental DSE relations, 2:  TEXT-BEARING SURFACE -> DEFAULT IMAGE *********** -->

    <index verb="dse:hasDefaultImage" inverse="dse:isDefaultImage">
        <source type="file" value="venA-defaultimg.csv"/>
    </index>


    <!--** end TEXT-BEARING SURFACE  -> DEFAULT IMAGES********************************************* -->








    <!-- ******* Fundamental DSE relations, 3:  TEXT -> IMAGE *********** -->
    <index verb="cite:illustratedBy" inverse="cite:illustrates">
        <source type="file" value="text2Image/venA-Iliad.csv"/>
    </index>







    <!--** end TEXT  -> IMAGE********************************************* -->






    <!-- ******* Fundamental DSE relations, 4:  TEXT-BEARING SURFACE -> ALL IMAGES *********** -->


    <index verb="cite:illustratedBy" inverse="cite:illustrates">
        <source type="file" value="venA-tbs2img.tsv"/>
    </index>


    <!--** end TEXT-BEARING SURFACE  -> ALL IMAGES********************************************* -->







    <!--** OTHER DATA SETS:  generic HMT categories  ********************************************* -->



    <index verb="hmt:commentsOn" inverse="hmt:commentedOnBy">
        <source type="file" value="scholiaToIliad-venA.csv"/>
    </index>


    <index verb="cite:illustratedBy" inverse="cite:illustrates">
        <source type="file" value="pageToImage-venA.csv"/>
    </index>



    <!--** end OTHER DATA SETS:  generic HMT categories********************************************* -->



    <!--** OTHER DATA SETS:  Venetus A********************************************* -->


    <index verb="dse:appearsOn" inverse="dse:hasOnIt">
        <source type="file" value="criticalsignToFolio.tsv"/>
    </index>
    <index verb="hmt:commentsOn" inverse="hmt:commentedOnBy">
        <source type="file" value="criticalsignToText.tsv"/>
    </index>

    <!--** end OTHER DATA SETS:  Venetus A********************************************* -->




</indexInventory>
