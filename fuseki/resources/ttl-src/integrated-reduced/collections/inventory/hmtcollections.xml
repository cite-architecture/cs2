<?xml version="1.0" encoding="UTF-8"?>
<?xml-model href="CiteCollectionInventory.rng" type="application/xml" schematypens="http://relaxng.org/ns/structure/1.0"?>
<collectionService
    xmlns="http://chs.harvard.edu/xmlns/cite"
    xmlns:dc="http://purl.org/dc/elements/1.1/">
    <extensionImplementation abbr="image" rdfType="cite:Image"/>
    
    
    <!-- IMAGE DATA ********************   -->
    
    
    
    <citeCollection canonicalId="Image" label="Label" urn="urn:cite:hmt:vaimg">
        <namespaceMapping
            abbr="hmt"
            fullValue="http://www.homermultitext.org/datans"/>
        <extension name="image"/>
        <title
            xmlns="http://purl.org/dc/elements/1.1/">Images of the Venetus A MS</title>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Christopher Blackwell</creator>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Neel Smith</creator>
        <coverage
            xmlns="http://purl.org/dc/elements/1.1/">All images from the Homer Multitext project's photography of the Venetus A manuscript.</coverage>
        <description
            xmlns="http://purl.org/dc/elements/1.1/"> Photography of the Venetus A manuscript by the Homer Multitext project.</description>
        <rights
            xmlns="http://purl.org/dc/elements/1.1/"> All data in this collection are available
            under the terms of the Creative Commons Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</rights>
        <source
            type="file"
            value="imgcollections/vaimgs.csv"/>
        <citeProperty name="Image" label="Image URN" type="citeurn"/>
        <citeProperty name="Label" label="Caption" type="string"/>
        <citeProperty name="Rights" label="Rights" type="string"/>
    </citeCollection>
    
    
    
    
    
    
    <!-- END IMAGE DATA ********************   -->
    
    
    
    <!-- CODEX MODELS:  CODICES AND PAGE ZONES ********************   -->
   


    <!-- CODEX MODELS:  ORDERED COLLECTIONS OF TEXT-BEARING SURFACES ********************   -->
    <citeCollection
        canonicalId="PageUrn"
        label="Label"
        urn="urn:cite:hmt:msA">
        <namespaceMapping
            abbr="hmt"
            fullValue="http://www.homermultitext.org/datans"/>
        <title
            xmlns="http://purl.org/dc/elements/1.1/">Pages of the Venetus A manuscript</title>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Christopher Blackwell</creator>
        <creator
            xmlns="http://purl.org/dc/elements/1.1/">Neel Smith</creator>
        <coverage
            xmlns="http://purl.org/dc/elements/1.1/">All folio sides of the Venetus A
            manuscript</coverage>
        <description
            xmlns="http://purl.org/dc/elements/1.1/">Folio sides of the Venetus A
            manuscript.</description>
        <rights
            xmlns="http://purl.org/dc/elements/1.1/"> All data in this collection are available
            under the terms of the Creative Commons Attribution-Non-Commercial 3.0 Unported License,
            http://creativecommons.org/licenses/by-nc/3.0/deed.en_US</rights>
        <orderedBy
            property="Sequence"/>
        <source
            type="file"
            value="venetusA.csv"/>
        <citeProperty
            label="URN"
            name="PageUrn"
            type="citeurn"/>
        <citeProperty
            label="Sequence"
            name="Sequence"
            type="number"/>
        <citeProperty
            label="Recto or verso"
            name="RV"
            type="string"/>
        <citeProperty
            label="description"
            name="Label"
            type="string"/>
    </citeCollection>
   
    <!-- END CODEX MODELS ************************************************* ********************   -->



